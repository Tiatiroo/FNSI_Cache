<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="270320242346" author="Vladimir">
        <createTable tableName="dictionaries">
            <column name="id" type="int" autoIncrement="true">
                <constraints  nullable="false" primaryKey="true"/>
            </column>
            <column name="system" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="code" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="display" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="280320240001" author="Vladimir" >
        <addUniqueConstraint
                columnNames="system, code, display"
                tableName="dictionaries"
        />
    </changeSet>
</databaseChangeLog>