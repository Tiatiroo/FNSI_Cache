<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="270320241952" author="Idkaev Roman">
        <createTable tableName="mappings">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="system" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="varchar(50)">
            </column>
            <column name="code" type="varchar(50)">
            </column>
            <column name="display" type="varchar(50)">
            </column>
        </createTable>
        <addUniqueConstraint
                columnNames="system, version"
                constraintName="system_version_mapping_uc"
                tableName="mappings"
        />
    </changeSet>

</databaseChangeLog>